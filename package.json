{
  "name": "promoted-ts-client",
  "version": "13.0.0",
  "description": "A Typescript Client to contact Promoted APIs.",
  "type": "module",
  "scripts": {
    "prettier": "prettier --write '**/*.{js,ts}' --ignore-path ./.prettierignore",
    "types": "tsc --noEmit",
    "build": "npm run build:clean && npm run build:lib",
    "build:clean": "rimraf dist",
    "build:lib": "rollup -c",
    "updatelink": "npm run build && cd dist && npm link",
    "clear_jest": "jest --clearCache",
    "unlink": "cd dist && npm unlink",
    "lint": "eslint './{src,app}/**/*.{ts,tsx}'",
    "test": "jest --coverage --silent",
    "test:watch": "jest --watchAll",
    "size": "size-limit",
    "finish": "jest --coverage && npm run lint && npm run types",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/promotedai/promoted-ts-client"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "promoted",
    "promoted.ai",
    "delivery",
    "metrics",
    "client",
    "nodejs",
    "typescript",
    "library"
  ],
  "author": {
    "name": "Dan Hill",
    "email": "dhill@promoted.ai",
    "url": "https://promoted.ai"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/promotedia/promoted-ts-client/issues"
  },
  "homepage": "https://github.com/promotedai/promoted-ts-client#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "npm run lint"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@size-limit/preset-small-lib": "^11.1.4",
    "@size-limit/webpack": "^11.1.4",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^48.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "rollup": "^4.18.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "semantic-release": "^24.0.0",
    "size-limit": "^11.1.4",
    "ts-jest": "^29.1.5",
    "typescript": "^5.5.3"
  }
}
